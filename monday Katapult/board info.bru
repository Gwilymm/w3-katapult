meta {
  name: board info
  type: graphql
  seq: 3
}

post {
  url: {{url}}
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{Bearer}}
}

body:graphql {
  query {
    boards(ids: 1550966743) {
      items_page(limit: 100) {
        items {
          id
          name
          column_values {
            id
            text
          }
        }
      }
    }
  }
  
}
